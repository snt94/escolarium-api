{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    
    "^.+\\.(ts|tsx)$": "ts-jest"
  },
  
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1",
    "^generated/(.*)$": "<rootDir>/generated/$1"
  }
}